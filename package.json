{
  "name": "sandboxie-js",
  "version": "1.0.2",
  "description": "Sandboxie CLI client",
  "keywords": [
    "sandboxie",
    "cli",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vladislav-puzyrev/sandboxie-js.git"
  },
  "license": "MIT",
  "author": "Vladislav Puzyrev",
  "type": "module",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "npm run clean:build && tsc --project tsconfig.build.json",
    "clean": "npm run clean:build && npm run clean:docs && npm run clean:coverage",
    "clean:build": "rimraf build",
    "clean:coverage": "rimraf coverage",
    "clean:docs": "rimraf docs",
    "coverage": "npm run clean:coverage && node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js --no-cache --coverage --testTimeout=20000",
    "docs": "npm run clean:docs && npm run docs:pull && npm run docs:typedoc && npm run docs:push",
    "docs:pull": "git clone https://github.com/vladislav-puzyrev/sandboxie-js.wiki.git docs && rimraf docs/!(.git)",
    "docs:push": "cd docs && git add . && git diff-index --quiet HEAD || git commit -m TypeDoc && git push",
    "docs:typedoc": "typedoc --theme github-wiki --excludePrivate --githubPages false --cleanOutputDir false src/index.ts",
    "verify": "npm run verify:types && npm run verify:lint && npm run verify:test",
    "verify:lint": "eslint .",
    "verify:test": "node --experimental-vm-modules --no-warnings node_modules/jest/bin/jest.js --no-cache --testTimeout=20000",
    "verify:types": "tsc --noEmit"
  },
  "dependencies": {
    "execa": "^7.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "eslint": "^8.43.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "typedoc": "^0.24.8",
    "typedoc-github-wiki-theme": "^1.1.0",
    "typedoc-plugin-markdown": "^3.15.3",
    "typescript": "^5.1.3"
  }
}
